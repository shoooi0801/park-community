<h1>投稿</h1>
<%= form_with model: @post do |f| %>

  <h4>公園名</h4>
  <%= f.text_field :name %>
  <h4>郵便番号</h4>
  <%= f.text_field :postal_code %>
  <h4>住所</h4>
  <%= f.text_field :address %>
  <h4>メモ</h4>
  <%= f.text_area :introduction %>
  <h4>駐車場</h4>
  <%= f.text_field :parking %><br>

  <%= f.submit '投稿' %>

<% end %>